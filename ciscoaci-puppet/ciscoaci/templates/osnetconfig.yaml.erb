network_config:
  -
    type: vlan
    device: <%= @aci_opflex_uplink_interface %>
    vlan_id: <%= @aci_apic_infravlan %>
    use_dhcp: true
    peerdns: false
    persistent_dhclient: true 
    mtu: 1600
    routes:
      -
        ip_netmask: 224.0.0.0/4
        next_hop: 0.0.0.0
        default: false
